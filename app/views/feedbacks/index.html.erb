<p id="notice"><%= notice %></p>

<h1>Listing Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Tx comentario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.tx_comentario %></td>
        <td><%= link_to 'Show', user_service_feedback_path(@user,@service, feedback) %></td>
        <td><%= link_to 'Edit', edit_user_service_feedback_path(@user,@service,feedback) %></td>
        <td><%= link_to 'Destroy', [@user,@service,feedback], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', user_services_path(@user) %>
