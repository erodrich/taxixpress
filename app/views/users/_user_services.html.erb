<h3>Ultimos Servicios</h3>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Fecha</th>
      <th>Hora</th>
      <th>Costo aproximado</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @services.each do |service| %>
      <tr>
        <td><%= service.co_confirmacion %></td>
        <td><%= service.fe_fecha.to_s %></td>
        <td><%= service.fe_hora.to_s(:time) %></td>
        <td><%= service.ss_costo_estimado %></td>
        <td><%= service.get_status(service).no_status %></td>
        <td><%= link_to 'Detalle', user_service_path(@user, service) %></td>
        <% if !service.get_feedback %>
          <td><%= link_to 'Feedback', new_user_service_feedback_path(@user,service) %></td>
        <% else %>
          <td><%= link_to 'Ver Comentario', user_service_feedback_path(@user,service, service.get_feedback) %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>