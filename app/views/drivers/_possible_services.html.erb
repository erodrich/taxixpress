<h3>Le pueden interesar estos servicios</h3>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Fecha</th>
      <th>Hora</th>
      <th>Costo aproximado</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @possible_services.each do |service| %>
      <tr>
        <td><%= service.co_confirmacion %></td>
        <td><%= service.fe_fecha.to_s %></td>
        <td><%= service.fe_hora.to_s(:time) %></td>
        <td><%= service.get_status(service).no_status %></td>
        <td><%= link_to 'Ver', driver_service_path(@driver, service) %></td>
      </tr>
    <% end %>
  </tbody>
</table>